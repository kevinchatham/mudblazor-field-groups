@inherits FieldBase<string, AddressFieldModel, MudTextField<string>>

<MudTextField Class="@Class"
              Style="@Style"
              Label="@Label"
              Value="Model.Value"
              ValueChanged="async (string input)=> await Changed(input.Trim())"
              Validation="Validator.Validate"
              For="@(() => Model.Value)"
              Variant="Variant"
              Disabled="Disabled"
              Immediate="Immediate"
              @ref="Field" />